{
  "name": "@mizchi/ai-toolkit",
  "version": "0.0.1",
  "nodeModulesDir": "auto",
  "tasks": {
    "dev": "deno run cli/mod.ts",
    "install-local": "deno install -Afg cli/mod.ts --name chat --config deno.json"
  },
  "imports": {
    "@ai-sdk/anthropic": "npm:@ai-sdk/anthropic@^1.2.7",
    "@ai-sdk/deepseek": "npm:@ai-sdk/deepseek@^0.2.7",
    "@ai-sdk/google": "npm:@ai-sdk/google@^1.2.9",
    "@ai-sdk/openai": "npm:@ai-sdk/openai@^1.3.9",
    "@david/dax": "jsr:@david/dax@^0.42.0",
    "@electric-sql/pglite": "npm:@electric-sql/pglite@^0.2.17",
    "@mizchi/readability": "npm:@mizchi/readability@^0.5.0",
    "@std/assert": "jsr:@std/assert@1",
    "ai": "npm:ai@^4.3.3",
    "drizzle-orm": "npm:drizzle-orm@^0.41.0"
  },
  "exports": {
    ".": "./core/mod.ts",
    "./cli": "./cli/mod.ts",
    "./tools": "./tools/mod.ts"
  },
  "exclude": ["examples"]
}
